.project-video-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(250px, 250px));
  justify-content: center;
  gap: 1rem;
  margin-top: 0.5rem;
  text-align: left;
}

.project-gallery-grid img {
  width: 100%;
  aspect-ratio: 1 / 1;
  object-fit: cover;
  border-radius: 5px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
  cursor: pointer;
  transition: transform 0.3s ease;
}

.project-gallery-grid .gallery-thumbnail:hover {
  transform: scale(1.05);
}

.gallery-link {
  display: block;
  overflow: hidden;
  border-radius: 5px;
}

.lightbox {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.85);
  z-index: 10000;
  display: none;
  justify-content: center;
  align-items: center;
}
.lightbox-active {
  display: flex;
}

.lightbox img {
  height: 80vh;
  width: auto;
  max-width: 90vw;
  object-fit: contain;
  border-radius: 5px;
  box-shadow: 0 0 30px rgba(0, 0, 0, 0.5);
}

.lightbox-close,
.lightbox-close:visited,
.lightbox-prev,
.lightbox-prev:visited,
.lightbox-next,
.lightbox-next:visited {
  position: absolute;
  font-size: 3rem;
  color: white;
  text-decoration: none;
  font-weight: bold;
  opacity: 0.7;
  transition: opacity 0.2s ease;
  z-index: 10001;
  cursor: pointer;
}

.lightbox-close:hover,
.lightbox-prev:hover,
.lightbox-next:hover {
  opacity: 1;
  color: white;
}

.lightbox-close {
  top: 20px;
  right: 30px;
}
.lightbox-prev {
  top: 50%;
  transform: translateY(-50%);
  left: 30px;
}
.lightbox-next {
  top: 50%;
  transform: translateY(-50%);
  right: 30px;
}

.video-wrapper {
  position: relative;
  padding-bottom: 56.25%;
  height: 0;
  overflow: hidden;
  margin-bottom: 1.25rem;
  text-align: left;
}
.video-wrapper iframe,
.video-wrapper video {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 5px;
}

/* --- STYLES FROM resources.html --- */
.document-view {
  max-width: 90vw;
  padding: 2rem;
  box-sizing: border-box;
  text-align: left;
  background-color: rgba(255, 255, 255, 0.9);
  border-radius: 8px;
}

.document-view hr {
  border: 0;
  height: 1px;
  background-color: #e8e8e8;
  margin: 1rem 0;
}

.document-view h3 {
  margin-top: 1rem;
}

.document-view a {
  color: #2a7ae2;
}

.document-view a:visited {
  color: #2a7ae2;
}

.document-view a:hover {
  color: #1756a9;
  text-decoration: underline;
}

.resources-background {
  background-image: url("/media/city.jpg");
  min-height: 80vh;
}
