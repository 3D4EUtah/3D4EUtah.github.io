<file path="_layouts/project_page.html">
---
layout: custom_page
---

<style>
  .project-header {
    width: 100%;
    text-align: center;
    background-color: black;
    color: white;
    padding: 3rem 1rem;
    box-sizing: border-box;
  }
  .project-header h1 {
    font-size: 2.5em;
    color: white;
    margin-bottom: 0.5rem;
  }
  .project-container {
    max-width: 90%;
    margin: 0 auto 2rem auto;
    text-align: center;
    padding: 1rem;
    box-sizing: border-box;
  }

  .project-description p {
    font-size: 1.2em;
    padding-top: 0;
  }

  .project-gallery h3,
  .project-videos h3 {
    border-bottom: 2px solid #eee;
    padding-bottom: 5px;
    display: inline-block;
    padding-left: 1rem;
    padding-right: 1rem;
    margin-bottom: 0.5rem;
  }

  /* --- UPDATED MEMBER LINK STYLING --- */
  .project-members p {
    font-size: 1.1em;
    margin-bottom: 5px;
  }

  /* This targets the link in all states (normal, visited, active) */
  .member-link,
  .member-link:visited,
  .member-link:active,
  .member-link:focus {
    color: #ba0c2f !important; /* Force University Red */
    font-weight: 600;
    text-decoration: none;
    transition: text-decoration 0.2s ease;
  }

  /* On hover, keep the red color but add the underline */
  .member-link:hover {
    color: #ba0c2f !important;
    text-decoration: underline;
  }
  /* ----------------------------------- */

  .project-description,
  .project-members,
  .project-gallery {
    margin-bottom: 1.5rem;
  }

  /* Gallery Grid */
  .project-gallery-grid,
  .project-video-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
    justify-content: center;
    gap: 1rem;
    margin-top: 0.5rem;
    text-align: left;
  }

  .project-gallery-grid {
    justify-items: center;
  }

  .project-gallery-grid img {
    width: 100%;
    height: 100%;
    aspect-ratio: 1 / 1;
    object-fit: cover;
    border-radius: 5px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
    cursor: pointer;
    transition: transform 0.3s ease;
  }

  .project-gallery-grid .gallery-thumbnail:hover {
    transform: scale(1.05);
  }

  /* Lightbox Styling */
  .lightbox {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.85);
    z-index: 10000;
    display: none;
    justify-content: center;
    align-items: center;
  }
  .lightbox-active {
    display: flex;
  }

  .lightbox img {
    height: 80vh;
    width: auto;
    max-width: 90vw;
    object-fit: contain;
    border-radius: 5px;
    box-shadow: 0 0 30px rgba(0, 0, 0, 0.5);
  }

  .lightbox-close,
  .lightbox-prev,
  .lightbox-next {
    position: absolute;
    font-size: 3rem;
    color: white;
    text-decoration: none;
    font-weight: bold;
    opacity: 0.7;
    transition: opacity 0.2s ease;
    z-index: 10001;
    cursor: pointer;
    background: none;
    border: none;
    padding: 0;
  }

  .lightbox-close:hover,
  .lightbox-prev:hover,
  .lightbox-next:hover {
    opacity: 1;
  }

  .lightbox-close {
    top: 20px;
    right: 30px;
  }
  .lightbox-prev {
    top: 50%;
    transform: translateY(-50%);
    left: 30px;
  }
  .lightbox-next {
    top: 50%;
    transform: translateY(-50%);
    right: 30px;
  }

  /* Video Wrapper */
  .video-wrapper {
    position: relative;
    padding-bottom: 56.25%;
    height: 0;
    overflow: hidden;
    margin-bottom: 1.25rem;
    text-align: left;
    width: 100%;
  }
  .video-wrapper iframe,
  .video-wrapper video {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-radius: 5px;
  }
</style>

<section class="title-section">
  <h1>{{ page.title }}</h1>
</section>

<div class="project-container">
  <section class="project-description">
    <p>{{ page.description }}</p>
    {{ content }}
  </section>

  {% if page.members and page.members.size > 0 %} {% unless page.members
  contains "Idea" %}
  <div class="project-members">
    <p>
      <strong>Team:</strong>
      {% for member_id in page.members %} {% assign member_page = site.members |
      where: "slug", member_id | first %} {% if member_page %}
      <a href="{{ member_page.url }}" class="member-link"
        >{{ member_page.title }}</a
      >{% unless forloop.last %}, {% endunless %} {% else %} {{ member_id }}{%
      unless forloop.last %}, {% endunless %} {% endif %} {% endfor %}
    </p>
  </div>
  {% endunless %} {% endif %} {% if page.images and page.images.size > 0 %}
  <section class="project-gallery">
    <h3>Gallery</h3>

    <div class="project-gallery-grid">
      {% assign gallery_path = "/media/projects/" %} {% for image_filename in
      page.images %} {% assign full_image_path = gallery_path | append:
      image_filename %}

      <img
        src="{{ full_image_path }}"
        alt="{{ page.title }} gallery image"
        class="gallery-thumbnail"
        data-index="{{ forloop.index }}"
      />
      {% endfor %}
    </div>

    <div class="lightbox-container">
      {% assign gallery_path = "/media/projects/" %} {% for image_filename in
      page.images %} {% assign full_image_path = gallery_path | append:
      image_filename %}

      <div class="lightbox lightbox-item" data-index="{{ forloop.index }}">
        <img src="{{ full_image_path }}" alt="{{ page.title }} gallery image" />

        <button
          class="lightbox-close js-lightbox-close"
          aria-label="Close image viewer"
        >
          &times;
        </button>
        <button
          class="lightbox-prev js-lightbox-prev"
          aria-label="Previous image"
        >
          &#10094;
        </button>
        <button class="lightbox-next js-lightbox-next" aria-label="Next image">
          &#10095;
        </button>
      </div>
      {% endfor %}
    </div>
  </section>
  {% endif %} {% if page.videos and page.videos.size > 0 %}
  <section class="project-videos">
    <h3>Videos</h3>
    <div class="project-video-grid">
      {% for video_src in page.videos %}
      <div class="video-wrapper">
        {% if video_src contains "http" %}
        <iframe
          src="{{ video_src }}"
          frameborder="0"
          allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
          allowfullscreen
        >
        </iframe>
        {% else %}
        <video controls>
          <source src="{{ video_src }}" type="video/mp4" />
          Your browser does not support the video tag.
        </video>
        {% endif %}
      </div>
      {% endfor %}
    </div>
  </section>
  {% endif %}
</div>

<script>
  document.addEventListener('DOMContentLoaded', function() {

    const totalImages = {{ page.images.size | default: 0 }};
    if (totalImages === 0) return;

    const thumbnails = document.querySelectorAll('.gallery-thumbnail');
    const lightboxes = document.querySelectorAll('.lightbox-item');
    const closeButtons = document.querySelectorAll('.js-lightbox-close');
    const prevButtons = document.querySelectorAll('.js-lightbox-prev');
    const nextButtons = document.querySelectorAll('.js-lightbox-next');

    let currentIndex = 0;

    function showLightbox(index) {
      if (index < 1 || index > totalImages) {
        return;
      }
      currentIndex = index;

      lightboxes.forEach(lb => lb.classList.remove('lightbox-active'));

      const activeLightbox = document.querySelector(`.lightbox-item[data-index="${index}"]`);
      if(activeLightbox) {
        activeLightbox.classList.add('lightbox-active');

        const prevButton = activeLightbox.querySelector('.js-lightbox-prev');
        const nextButton = activeLightbox.querySelector('.js-lightbox-next');

        if (prevButton) prevButton.style.display = (index === 1) ? 'none' : 'block';
        if (nextButton) nextButton.style.display = (index === totalImages) ? 'none' : 'block';
      }
    }

    function closeLightbox() {
      lightboxes.forEach(lb => lb.classList.remove('lightbox-active'));
    }

    function showNext() {
      showLightbox(currentIndex + 1);
    }

    function showPrev() {
      showLightbox(currentIndex - 1);
    }

    thumbnails.forEach(thumb => {
      thumb.addEventListener('click', function() {
        showLightbox(parseInt(this.dataset.index));
      });
    });

    closeButtons.forEach(btn => btn.addEventListener('click', closeLightbox));
    prevButtons.forEach(btn => btn.addEventListener('click', (e) => { e.stopPropagation(); showPrev(); }));
    nextButtons.forEach(btn => btn.addEventListener('click', (e) => { e.stopPropagation(); showNext(); }));

    lightboxes.forEach(lightbox => {
      lightbox.addEventListener('click', function(event) {
        if (event.target === this) {
          closeLightbox();
        }
      });
    });

    document.addEventListener('keydown', function(event) {
      if (!document.querySelector('.lightbox-active')) {
        return;
      }

      if (event.key === 'Escape' || event.key === 'Esc') {
        closeLightbox();
      }
      if (event.key === 'ArrowRight') {
        showNext();
      }
      if (event.key === 'ArrowLeft') {
        showPrev();
      }
    });

  });
</script>
</file>

