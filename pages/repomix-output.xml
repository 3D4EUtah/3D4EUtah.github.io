/* --- Styles for New Project Grid --- */

/* 1. This defines the responsive grid container */
.project-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
  gap: 1.5rem;
  padding: 1.5rem;

  /* FIX: Makes the grid container fill its parent */
  width: 100%;
  box-sizing: border-box;
}

/* 2. This overrides old .project-box styles when inside the grid */
.project-grid .project-box {
  /* Resets the old width/max-width from the scroll-box style */
  max-width: none;
  width: 100%;

  /* --- MODIFIED/ADDED FOR CENTERING --- */
  display: flex;
  flex-direction: column;
  justify-content: center; /* This centers vertically */
  min-height: 75px; /* Gives "Future" boxes a good height */
  /* ------------------------------------ */

  margin-right: 0;
  vertical-align: baseline;
  box-sizing: border-box;
}

/* 3. This ensures images scale correctly inside the boxes */
.project-box img {
  max-width: 100%;
  height: auto;
  display: block;
}
/* --- Styles for Projects Page (Fixed Specificity) --- */

/* This rule is now specific enough to override the old button styles */
.project-box a.project-image-link {
  display: block;
  overflow: hidden;
  max-width: 100%;
  border-radius: 8px;
  margin: 0 auto 15px auto;

  /* Overrides for the old .project-box a */
  background-color: transparent;
  padding: 0;
  border: none;
  outline: none;
  text-decoration: none;
}

/* This overrides the red hover/focus states */
.project-box a.project-image-link:hover,
.project-box a.project-image-link:focus,
.project-box a.project-image-link:active {
  background-color: transparent;
  outline: none;
  border: none;
  text-decoration: none;
}

/* This overrides the default .project-box img styles */
.project-box a.project-image-link img {
  max-width: 100%;
  width: 100%;
  margin: 0;
  margin-bottom: 0;
  transition: transform 0.3s ease;
}

/* This is the zoom-on-hover effect */
.project-box a.project-image-link:hover img {
  transform: scale(1.1);
}

